{
  "name": "friendsofsymfony1/cache-legacy-bridge",
  "type": "library",
  "description": "Cache bridge to use modern Symfony cache systems in legacy Symfony1 projects",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "friendsofsymfony1/symfony1": "^1.4",
    "symfony/cache": "^4.3 || ^5.0"
  },
  "require-dev": {
    "ext-apcu": "*"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Symfony1\\LegacyBridge\\Cache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Symfony1\\LegacyBridge\\Cache\\": "tests/"
    }
  }
}
